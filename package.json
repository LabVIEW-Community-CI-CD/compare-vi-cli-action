{
  "name": "compare-vi-cli-action",
  "private": true,
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "derive:env": "node dist/src/env/derive-env.js --json",
    "schema:watcher:validate": "ajv validate -s docs/schema/watcher-telemetry.v1.schema.json -d tests/results/_agent/handoff/watcher-telemetry.json --spec=draft2020 -c ajv-formats",
    "generate:outputs": "node dist/generate-action-outputs.js",
    "schemas:generate": "tsc -p tsconfig.json && node dist/schemas/generate-schemas.js",
    "tests:discover": "tsc -p tsconfig.json && node dist/test-discovery.js --tests tests --out tests/results/_agent/test-manifest.json",
    "lint:md": "pwsh -NoLogo -NoProfile -File tools/Lint-Markdown.ps1 -All",
    "lint:md:changed": "pwsh -NoLogo -NoProfile -File tools/Lint-Markdown.ps1",
    "watch:pester": "node tools/follow-pester-artifacts.mjs --results tests/results",
    "watch:pester:fast": "node tools/follow-pester-artifacts.mjs --results tests/results --warn-seconds 60 --hang-seconds 120",
    "watch:pester:fast:exit": "node tools/follow-pester-artifacts.mjs --results tests/results --warn-seconds 60 --hang-seconds 120 --poll-ms 2000 --no-progress-seconds 60 --exit-on-hang --exit-on-no-progress",
    "watch:pester:busyexit": "node tools/follow-pester-artifacts.mjs --results tests/results --no-progress-seconds 90 --poll-ms 2000 --exit-on-no-progress",
    "watch:pester:ps": "pwsh -NoLogo -NoProfile -File tools/Follow-PesterArtifacts.ps1 -ResultsDir tests/results -WarnSeconds 60 -HangSeconds 120",
    "watch:pester:ps:exit": "pwsh -NoLogo -NoProfile -File tools/Follow-PesterArtifacts.ps1 -ResultsDir tests/results -WarnSeconds 60 -HangSeconds 120 -NoProgressSeconds 60 -PollMs 2000 -ExitOnHang -ExitOnNoProgress",
    "dev:watcher:ensure": "pwsh -NoLogo -NoProfile -File tools/Dev-WatcherManager.ps1 -Ensure",
    "dev:watcher:status": "pwsh -NoLogo -NoProfile -File tools/Dev-WatcherManager.ps1 -Status",
    "dev:watcher:stop": "pwsh -NoLogo -NoProfile -File tools/Dev-WatcherManager.ps1 -Stop",
    "dev:watcher:trim": "pwsh -NoLogo -NoProfile -File tools/Dev-WatcherManager.ps1 -Trim",
    "dev:watcher:autotrim": "pwsh -NoLogo -NoProfile -File tools/Dev-WatcherManager.ps1 -AutoTrim",
    "ci:status": "pwsh -NoLogo -NoProfile -File tools/Follow-OrchestratedRun.ps1",
    "ci:watch": "pwsh -NoLogo -NoProfile -File tools/Follow-OrchestratedRun.ps1 -Watch",
    "session-index:sample": "tsc -p tsconfig.json && node dist/src/session-index/cli.js --sample",
    "hooks:pre-commit": "node tools/hooks/core/pre-commit.mjs",
    "hooks:pre-push": "node tools/hooks/core/pre-push.mjs",
    "hooks:plane": "node tools/hooks/core/plane-info.mjs",
    "hooks:preflight": "node tools/hooks/core/preflight.mjs",
    "hooks:multi": "node tools/hooks/core/run-multi.mjs",
    "hooks:schema": "node tools/hooks/core/validate-summaries.mjs",
    "hooks:test": "node --test \"tools/hooks/__tests__/*.mjs\""
  },
  "devDependencies": {
    "@types/argparse": "^2.0.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.11.30",
    "ajv": "8.16.0",
    "ajv-cli": "5.0.0",
    "ajv-formats": "2.1.1",
    "js-yaml": "4.1.0",
    "markdownlint-cli2": "0.14.0",
    "typescript": "5.4.5"
  },
  "description": "<!-- ci: bootstrap status checks -->",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "braces": "^3.0.3",
    "chokidar": "^3.6.0",
    "entities": "^4.5.0",
    "fast-glob": "^3.3.3",
    "fastq": "^1.19.1",
    "fill-range": "^7.1.1",
    "glob-parent": "^5.1.2",
    "globby": "^14.0.2",
    "ignore": "^5.3.2",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "jsonc-parser": "^3.3.1",
    "linkify-it": "^5.0.0",
    "markdown-it": "^14.1.0",
    "markdownlint": "^0.35.0",
    "markdownlint-cli2-formatter-default": "^0.0.5",
    "markdownlint-micromark": "^0.1.10",
    "mdurl": "^2.0.0",
    "merge2": "^1.4.1",
    "micromatch": "^4.0.8",
    "path-type": "^5.0.0",
    "picomatch": "^2.3.1",
    "punycode.js": "^2.3.1",
    "queue-microtask": "^1.2.3",
    "reusify": "^1.1.0",
    "run-parallel": "^1.2.0",
    "slash": "^5.1.0",
    "to-regex-range": "^5.0.1",
    "uc.micro": "^2.1.0",
    "undici-types": "^5.26.5",
    "unicorn-magic": "^0.1.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabVIEW-Community-CI-CD/compare-vi-cli-action.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/LabVIEW-Community-CI-CD/compare-vi-cli-action/issues"
  },
  "homepage": "https://github.com/LabVIEW-Community-CI-CD/compare-vi-cli-action#readme"
}
