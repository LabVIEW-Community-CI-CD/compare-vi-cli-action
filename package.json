{
  "name": "compare-vi-cli-action",
  "private": true,
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate:outputs": "node dist/generate-action-outputs.js",
    "lint:md": "markdownlint-cli2 \"**/*.md\" --config .markdownlint.jsonc || exit 1",
    "watch:pester": "node tools/follow-pester-artifacts.mjs --results tests/results",
    "watch:pester:fast": "node tools/follow-pester-artifacts.mjs --results tests/results --warn-seconds 60 --hang-seconds 120",
    "watch:pester:fast:exit": "node tools/follow-pester-artifacts.mjs --results tests/results --warn-seconds 60 --hang-seconds 120 --poll-ms 2000 --exit-on-hang",
    "watch:pester:ps": "pwsh -NoLogo -NoProfile -File tools/Follow-PesterArtifacts.ps1 -ResultsDir tests/results -WarnSeconds 60 -HangSeconds 120",
    "watch:pester:ps:exit": "pwsh -NoLogo -NoProfile -File tools/Follow-PesterArtifacts.ps1 -ResultsDir tests/results -WarnSeconds 60 -HangSeconds 120 -PollMs 2000 -ExitOnHang"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "20.11.30",
    "js-yaml": "4.1.0",
    "markdownlint-cli2": "0.14.0",
    "typescript": "5.4.5"
  },
  "description": "<!-- ci: bootstrap status checks -->",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "chokidar": "^3.6.0",
    "braces": "^3.0.3",
    "entities": "^4.5.0",
    "fast-glob": "^3.3.3",
    "fastq": "^1.19.1",
    "fill-range": "^7.1.1",
    "glob-parent": "^5.1.2",
    "globby": "^14.0.2",
    "ignore": "^5.3.2",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "jsonc-parser": "^3.3.1",
    "linkify-it": "^5.0.0",
    "markdown-it": "^14.1.0",
    "markdownlint": "^0.35.0",
    "markdownlint-cli2-formatter-default": "^0.0.5",
    "markdownlint-micromark": "^0.1.10",
    "mdurl": "^2.0.0",
    "merge2": "^1.4.1",
    "micromatch": "^4.0.8",
    "path-type": "^5.0.0",
    "picomatch": "^2.3.1",
    "punycode.js": "^2.3.1",
    "queue-microtask": "^1.2.3",
    "reusify": "^1.1.0",
    "run-parallel": "^1.2.0",
    "slash": "^5.1.0",
    "to-regex-range": "^5.0.1",
    "uc.micro": "^2.1.0",
    "undici-types": "^5.26.5",
    "unicorn-magic": "^0.1.0"
  },
  "repository": {
    "type": "git",
